{
  "name": "stencil-env-swap",
  "version": "1.0.0",
  "description": "A lightweight tool to allow for easy switching of BigCommerce environments for local development with Stencil CLI",
  "main": "index.js",
  "scripts": {
    "swap:dev": "APP_ENV=dev npx ./env/setEnv.js",
    "swap:stage": "APP_ENV=stage node ./env/setEnv.js",
    "swap:prod": "APP_ENV=production node ./env/setEnv.js"
  },
  "keywords": [
    "bigcommerce",
    "bigcommerce-env",
    "env-swap",
    "environment",
    "stencil-cli",
    "stencil-swap"
  ],
  "author": "Kimberly Sheamon <kimberly.sheamon@gmail.com>",
  "bugs": {
    "url": "https://github.com/ksheamon/stencil-env-swap/issues"
  },
  "homepage": "https://github.com/ksheamon/stencil-env-swap",
  "repository": "https://github.com/ksheamon/stencil-env-swap",
  "license": "ISC",
  "type": "module",
  "devDependencies": {
    "child_process": "^1.0.2",
    "dotenv": "^16.0.3"
  }
}
